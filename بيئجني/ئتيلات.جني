ئورد الكل ك رياضي من ئساسية.رياضيات؛

ئعلن ئستولدرمزشركة () ك نصية:
	نصية[] حرفين = ['ج'، 'ح'، 'خ'، 'د'، 'ر'، 'ز'، 'س'، 'ش'، 'ص'، 'ض'، 'ط'، 'ع'، 'غ'، 'ك'، 'ل'، 'م'، 'ن'، 'و'، 'ي']؛
	نصية[] رقمين = ['1'، '2'، '3'، '4'، '5'، '6'، '7'، '8'، '9']؛
	نصية[] رمز = [
		رقمين[رياضي.ئستصغر(رياضي.عشوائي() × رقمين.طول)]،
		رقمين[رياضي.ئستصغر(رياضي.عشوائي() × رقمين.طول)]،
		حرفين[رياضي.ئستصغر(رياضي.عشوائي() × رقمين.طول)]،
		رقمين[رياضي.ئستصغر(رياضي.عشوائي() × رقمين.طول)]،
		حرفين[رياضي.ئستصغر(رياضي.عشوائي() × رقمين.طول)]،
		رقمين[رياضي.ئستصغر(رياضي.عشوائي() × رقمين.طول)]
	]؛
	ئرجع رمز[0]؛ #.ئدمج('')؛
تم

ئعلن ئستولدمعرف () ك نصية:
	تاريخ تخ = تاريخ()؛
	ئرجع "_{تخ.ردوقت()}"؛
تم

ئعلن ئستولدرمزتحقق (نصية ئلا) ك نصية:
	عدد رمز = رياضي.ئستقرب(رياضي.عشوائي() × 9000 + 1000)؛
	قل رمز؛
	ئرجع "_{رمز}"؛
تم
