ئورد بتشدبي من مكون.بتشدبي؛
ئورد الكل ك سياق من سياق؛

ئعلن _ئلارمزلاتيني (نصية رمز) ك نصية:
	نصية[] عربي = ["ج"، "ح"، "خ"، "د"، "ر"، "ز"، "س"، "ش"، "ص"، "ض"، "ط"، "ع"، "غ"، "ك"، "ل"، "م"، "ن"، "و"، "ي"]؛
	نصية[] لات = ["j"، "h"، "w"، "d"، "r"، "z"، "s"، "x"، "c"، "y"، "t"، "a"، "v"، "k"، "l"، "m"، "n"، "u"، "i"]؛
	لكل ع في عربي:
		رمز = رمز.عووض(عربي[ع]، لات[ع])؛
	تم
	ئرجع رمز؛
تم


ئعلن ردقاب (نصية رمز) ك وعد نوعبنية:
	دع قابمحلي = بتشدبي (سياق.قابمحلي)؛
	دع ئسمقاب = 'jinni_' + _ئلارمزلاتيني(رمز)؛
	#دع ئسمقاب = "jinni_{ئلارمزلاتيني(رمز)}"؛
	#دع ولوجيات = ئجرائ.بيئة.قابمستخدم + ":" + ئجرائ.بيئة.قابمرور؛
	#دع لول = بيئة.قابمستخدم؛
	دع ولوجيات = "{سياق.قابمستخدم}:{سياق.قابمرور}"؛
	دع قابعيد = بتشدبي ("https://{ولوجيات}@{سياق.قابنطاق}/{ئسمقاب}")؛
	دع نتج = ريتما قابعيد.معلو()؛
	ئرجع {
		محلي: قابمحلي،
		بعيد: قابعيد
	}؛
تم


ئعلن ردبيا (مدخ) ك نوعبنية:
	# تنصيب الشركة
	دع شركة = {
		_معرف: مدخ.رمزشركة،
		نوع: "شركة"،
		رمزشركة: مدخ.رمزشركة،
		ئسم: مدخ.ئدخئسمشركة،
		مدينة: مدخ.ئدخمدينة،
		دولة: مدخ.ئدخبلد،
		موقعويب: مدخ.ئدخموقع
	}؛
تم
